@model FinancePortal.Models.HouseholdAttachment

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Household Attachment</h4>
    <hr />
    <div class="row">
        <div class="col-6">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title text-success">Household Attachment</h4>
                    <dl class="row">
                        <dt class="col-sm-2">
                            File Name
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.FileName)
                        </dd>
                        <dt class="col-sm-2">
                            Description
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.Description)
                        </dd>
                        <dt class="col-sm-2">
                            Created
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.Created)
                        </dd>
                        <dt class="col-sm-2">
                            Content Type
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.ContentType)
                        </dd>
                        <dt class="col-sm-2">
                            File Data
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.FileData)
                        </dd>
                    </dl>
                </div>
            </div>
        </div>
    </div>
</div>
<div>
    <a class="btn btn-outline-success" asp-action="Edit" asp-route-id="@Model.Id">Edit</a>
    <a class="btn btn-outline-primary" asp-controller="Households" asp-action="Dashboard">Back to List</a>
</div>
